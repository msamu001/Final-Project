import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

public class EWG_Test {
	EWG graph;
	@Before
	public void setUp() throws Exception {
		graph = new EWG();
		graph.addVertex("a");
	}

	@Test
	public void test() {
		assert.assertNotEquals(null, graph.getVertex("a"));
	}
}